{
  "name": "echo-registry",
  "private": true,
  "main": "src/echos.js",
  "type": "module",
  "engine": "20.17",
  "scripts": {
    "build:codegen": "tsx src/codegen.ts",
    "build:prettier": "pretty-quick --check --pattern 'src/**/*.{js,json,ts}'",
    "build:typecheck": "tsc --noEmit",
    "build": "npm run build:codegen && npm run build:prettier && npm run build:typecheck",
    "start": "vercel dev --listen 4000",
    "clean": "sh ./scripts/clean.sh"
  },
  "keywords": [],
  "author": "Kier Borromeo <kier@stridelabs.co>",
  "dependencies": {
    "@vercel/node": "^3.2.27",
    "prettier": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vercel": "^39.1.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "@types/prettier": "^3.0.0",
    "pretty-quick": "^4.0.0"
  }
}
